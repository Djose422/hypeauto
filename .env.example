# Clave secreta para autenticar peticiones desde jadhstore
API_SECRET_KEY=tu-clave-secreta-aqui

# Puerto del servidor
PORT=8000

# Número máximo de navegadores concurrentes
MAX_CONCURRENT=3

# Webhook URL para notificar a jadhstore cuando se complete una redención (opcional)
WEBHOOK_URL=https://tu-jadhstore.onrender.com/api/webhook/hype

# Modo headless del navegador (true para VPS, false para debug local)
HEADLESS=true

# Timeout en segundos para cada redención
REDEEM_TIMEOUT=60
